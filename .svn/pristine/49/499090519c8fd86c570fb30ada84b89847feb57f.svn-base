//Sebastien

table 
{
	background: -webkit-linear-gradient(top, #EAEAEA, #BBBBBB);
        background: -moz-linear-gradient(top, #EAEAEA, #BBBBBB);
        margin: 10px;
}	

th 
{
	background: -moz-linear-gradient(top,  #997711 0%, #FFBC21 100%); 
        background: -webkit-linear-gradient(top,  #997711 0%,#FFBC21 100%);
	border-left: 1px solid #555;
	border-right: 1px solid #777;
	border-top: 1px solid #555;
	border-bottom: 1px solid #333;
	color: #fff;
        font-weight: bold;
	padding: 10px 15px;
	position: relative;
	text-shadow: 0 1px 0 #000;	
}

th:after 
{
	background: linear-gradient(rgba(255,255,255,0), rgba(255,255,255,.08));
	content: '';
	display: block;
	height: 25%;
	left: 0;
	margin: 1px 0 0 0;
	position: absolute;
	top: 25%;
	width: 100%;
}

td {
	border-right: 1px solid #fff;
	border-left: 1px solid #e8e8e8;
	border-top: 1px solid #fff;
	border-bottom: 1px solid #e8e8e8;
	padding: 10px 15px;
	position: relative;
	transition: all 300ms;
}

td:first-child 
{
	box-shadow: inset 1px 0 0 #fff;
}	

td:last-child 
{
	border-right: 1px solid #e8e8e8;
	box-shadow: inset -1px 0 0 #fff;
}
.searchButton
{
    color: #000000;
    transition: box-shadow 0.5s ease;
    margin: 5px auto;
    padding: 10px 2px;
    box-shadow: 0px 0px 0px;
    border: solid 5px #EEEEEE;
    text-transform: uppercase;
    background-color: #EEEEEE;
    cursor: pointer;
    float:right;
    //font-size: 120%
}
.searchContainer
{
    height:auto;
    width:auto;
    position:absolute;
    right:30px;
    top:10px;
    
}
.searchButton:hover
{
    box-shadow: 0px 0px 10px;
}

.prettytextboxsearch
{
    display: block;
    margin: 5px auto;
    padding: 10px;
    border: solid 5px white;
    transition: border 0.3s;
    width: 280px;
    float: right;
}
.prettytextboxsearch:focus, .prettytextbox:hover
{
    border: solid 5px rgba(0, 0, 0, 0.6);
}
.prettytextboxusername
{
    display: block;
    margin-right:auto;
    margin-left:auto;
    margin-bottom:3px;
    padding: 3px;
    border: solid 5px white;
    transition: border 0.3s;
    width: 220px;
    //float: left;
}
.prettytextboxusername:focus, .prettytextbox:hover
{
    border: solid 5px rgba(0, 0, 0, 0.6);
}

