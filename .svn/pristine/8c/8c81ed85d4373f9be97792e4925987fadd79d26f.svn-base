package domains;

/**
 *
 * @author sebaslab
 */
public class Gallery {
    private int idGallery;
    private String title;

    /**
     *
     */
    public Gallery() {
    }

    /**
     *
     * @param idGallery
     * @param title
     */
    public Gallery(int idGallery, String title) {
        this.idGallery = idGallery;
        this.title = title;
    }

    /**
     *
     * @return
     */
    public int getIdGallery() {
        return idGallery;
    }

    /**
     *
     * @param idGallery
     */
    public void setIdGallery(int idGallery) {
        this.idGallery = idGallery;
    }

    /**
     *
     * @return
     */
    public String getTitle() {
        return title;
    }

    /**
     *
     * @param title
     */
    public void setTitle(String title) {
        this.title = title;
    }

    @Override
    public String toString() {
        return "Gallery{" + "idGallery=" + idGallery + ", title=" + title + '}';
    }
    
    
}
