package domains;

/**
 *
 * @author sebaslab
 */
public class Tag {
    private String name;

    /**
     *
     */
    public Tag() {
    }

    /**
     *
     * @param name
     */
    public Tag(String name) {
        this.name = name;
    }

    /**
     *
     * @return
     */
    public String getName() {
        return name;
    }

    /**
     *
     * @param name
     */
    public void setName(String name) {
        this.name = name;
    }

    @Override
    public String toString() {
        return "Tag{" + "name=" + name + '}';
    }
    
    
}
